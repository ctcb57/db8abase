@model db8abase.Models.Debater

@{
    ViewData["Title"] = "Details";
}
<br />
<h1>Debater Details</h1>

<div>
 
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.LastName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt class = "col-sm-2">
            Avg. Speaker Points
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AnnualAverageSpeakerPoints)
        </dd>
    </dl>
</div>
<div>
    <a asp-controller="Coaches" asp-action="ManageTeam">Back to List</a>
</div>
